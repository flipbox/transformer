{
  "name": "flipboxdigital/transformer",
  "description": "Manipulate complex data structures",
  "version": "1.0.0-beta",
  "require": {
    "flipboxdigital/transform": "^1.0"
  },
  "repositories": [{
    "type": "composer",
    "url": "https://asset-packagist.org"
  }],
  "autoload": {
    "psr-4": {
      "flipbox\\transformer\\": "src/"
    }
  },
  "extra": {
    "name": "Transformer",
    "handle": "transformer",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "class": "flipbox\\transformer\\Transformer",
    "schemaVersion": "1.0.0",
    "components": {
      "transformer": "flipbox\\transformer\\services\\Transformer"
    },
    "modules": {
      "field": {
        "basePath": "@flipbox/transformer/modules/field",
        "class": "flipbox\\transformer\\modules\\field\\Module",
        "components": {
          "transformer": "flipbox\\transformer\\modules\\field\\services\\Transformer"
        }
      },
      "element": {
        "basePath": "@flipbox/transformer/modules/element",
        "class": "flipbox\\transformer\\modules\\element\\Module",
        "components": {
          "transformer": "flipbox\\transformer\\modules\\element\\services\\Transformer"
        }
      },
      "model": {
        "basePath": "@flipbox/transformer/modules/model",
        "class": "flipbox\\transformer\\modules\\model\\Module",
        "components": {
          "transformer": "flipbox\\transformer\\modules\\model\\services\\Transformer"
        }
      }
    }
  }
}
